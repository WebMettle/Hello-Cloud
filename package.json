{
  "name": "hello-cloud",
  "version": "1.0.0",
  "description": "Hello Cloud project with an HTMX front-end.",
  "homepage": "https://github.com/WebMettle/Hello-Cloud#readme",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "node --test",
    "t:HelloDate": "node --test javascript/HelloDate.test.js",
    "devServer": "HelloCloudListenPort=3100 node --watch-preserve-output --watch-path=./javascript --watch-path=./views --watch-path=./public app.js",
    "devSync": "npx browser-sync start --proxy 'http://localhost:3100' --port 3101 --ui-port 3110 --files 'app.js, javascript/*.js, views/*.js, public/*.css'",
    "prodStart": "HelloCloudListenPort=3100 node app.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WebMettle/Hello-Cloud.git"
  },
  "keywords": [],
  "author": "WebMettle Systems",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/WebMettle/Hello-Cloud/issues"
  },
  "dependencies": {
    "express": "^5.1.0"
  },
  "devDependencies": {
    "browser-sync": "^3.0.4"
  }
}
